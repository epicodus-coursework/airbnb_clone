<% if @properties.any? %>
  <% @properties.each do |property| %>
    <ul class="list-unstyled">
      <li>
        <%= link_to image_tag(property.photo.url(:thumb)), property_path(property) %>
        <%= link_to property.name, property_path(property) %>
        $<%= property.price %>
      </li>
    </ul>
  <% end %>
<% end %>
